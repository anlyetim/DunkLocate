<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient android:startColor="@color/orange"
                android:endColor="@color/main_background"
                android:angle="315"/>
        </shape>
    </item>
     <item android:bottom="3dp" android:top="3dp"
    android:right="3dp"
    android:left="3dp">
    <shape android:shape="oval">
        <solid android:color="@color/main_background"/>
    </shape>
</item>
    <item android:drawable="@drawable/location"
    android:top="15dp" android:bottom="15dp"
    android:right="15dp" android:left="15dp"/>
</layer-list>
